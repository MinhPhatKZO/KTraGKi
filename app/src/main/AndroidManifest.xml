<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfood_nguyenminhphat">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFood_NguyenMinhPhat"
        >

        <!-- LoginActivity: Là màn hình khởi động -->
        <activity
            android:name=".LoginActivity_MinhPhat"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- LogoutActivity: sau khi đăng nhập thành công -->
        <activity
            android:name=".LogoutActivity_MinhPhat"
            android:exported="false" />
        <activity
            android:name=".HomeActivity_MinhPhat"
            android:exported="false" />

    </application>


</manifest>
